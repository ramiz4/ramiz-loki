{
  "name": "ramiz-loki",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://ramiz4.github.io/ramiz-loki",
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "pnpm vite",
    "build": "pnpm vite build",
    "lint": "pnpm eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fix-imports": "node fix-imports.js",
    "format": "node fix-imports.js && pnpm eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" && pnpm prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "preview": "pnpm vite preview",
    "test": "pnpm cross-env NODE_OPTIONS=--no-deprecation jest",
    "test:watch": "pnpm cross-env NODE_OPTIONS=--no-deprecation jest --watch",
    "test:coverage": "pnpm cross-env NODE_OPTIONS=--no-deprecation jest --coverage",
    "predeploy": "pnpm run build",
    "deploy": "pnpm gh-pages -d dist"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^12.7.0",
    "lucide-react": "^0.323.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "latest",
    "canvas": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "intersection-observer": "^0.12.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "latest",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}